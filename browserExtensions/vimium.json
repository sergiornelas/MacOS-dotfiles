{
  "name": "Vimium C",
  "autoDarkMode": 1,
  "exclusionRules": [
    {
      "passKeys": "<esc>",
      "pattern": ":https://codepen.io/"
    },
    {
      "passKeys": "w",
      "pattern": ":https://codepen.io/"
    }
  ],
  "keyLayout": 2,
  "keyMappings": [
    "map a focusInput",
    "map c LinkHints.activateOpenUrl",
    "map e LinkHints.activateOpenInNewTab",
    "map gY autoOpen",
    "map ga LinkHints.activateEdit",
    "map gc LinkHints.click",
    "map gh LinkHints.activateHover",
    "map g` joinTabs",
    "map gp togglePinTab",
    "map gx LinkHints.activateFocus",
    "map gy searchAs",
    "map q LinkHints.activateWithQueue",
    "map r reloadTab",
    "map s LinkHints.activateSelect",
    "map w removeTab",
    "map x closeSomeOtherTabs",
    "map yI LinkHints.activateDownloadImage",
    "map yl LinkHints.activateCopyLinkUrl",
    "map zi zoomIn",
    "map zo zoomOut",
    "map zr zoomReset",
    "map D goForward",
    "map E previousTab",
    "map F visitPreviousTab",
    "map H moveTabLeft",
    "map J goBack",
    "map K goForward",
    "map L moveTabRight",
    "map R nextTab",
    "map S goBack",
    "map Y autoCopy",
    "map << closeTabsOnLeft",
    "map >> closeTabsOnRight",
    "map ; passNextKey",
    "map ' Marks.activate",
    "map <c-g> visitPreviousTab"
  ],
  "linkHintCharacters": "jiofewakldspq",
  "smoothScroll": false,
  "userDefinedCss": ["/* #omni */", "#bar { background: #4F0A0A; }", ""],
  "vimSync": true
}
